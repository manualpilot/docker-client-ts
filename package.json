{
  "name": "docker-client-ts",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "packageManager": "pnpm@9.10.0",
  "engines": {
    "node": ">=22",
    "pnpm": ">=9"
  },
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "esbuild --sourcemap --format=esm --platform=node --outdir=dist --log-level=debug './src/**/*.ts'",
    "postbuild": "tsc",
    "format": "biome check --verbose --write --unsafe",
    "generate": "node ./generator/generator.mjs",
    "postgenerate": "pnpm run format",
    "test": "jest"
  },
  "dependencies": {
    "undici": "^6.19.8",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^11.7.0",
    "@biomejs/biome": "^1.9.3",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.4",
    "@types/nunjucks": "^3.2.6",
    "esbuild": "^0.24.0",
    "jest": "^29.7.0",
    "json-schema-to-zod": "^2.4.1",
    "nunjucks": "^3.2.4",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.2",
    "yaml": "^2.5.1"
  }
}
